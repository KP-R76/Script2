game.StarterGui:SetCore("SendNotification", {
    Title = "VTGSSR / KP-R76";
    Text = "GUI BETA BY VTGSSR"; -- what the text says (ofc)
    Duration = 30;
})

if game.PlaceId == 11636413564 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/Revenant", true))()
    local Flags = Library.Flags
    --Library.DefaultColor = Color3.fromRGB(65, 143, 232)

    local Window = Library:Window({
        Text = "Meme Merger | KP-R76"
    })

    local plr = game.Players.LocalPlayer
    local plot = nil
    local obby = game:GetService("Workspace").Obbies
    for i,v in pairs(game:GetService("Workspace").Plots:GetDescendants()) do
        if v:IsA("ObjectValue") and v.Value == plr then
            plot = v.Parent.Name
        end
    end
    getgenv().thoigian = false
    getgenv().maxkhoi = false
    getgenv().capkhoi = false
    getgenv().autorebirth = false
    getgenv().autoclickblock = false
    getgenv().AutoMerge = false
    -- function
    -- [[
    function am()
        spawn(function()
            while task.wait() and getgenv().AutoMerge do
                for i, v in pairs(game:GetService("Workspace").Plots[plot].Merging:GetChildren()) do
                    v.CFrame = game:GetService("Workspace").Plots[plot].AreaOwnerStand.Owner.CFrame * CFrame.new(0,0,10)
                end
            end
        end)
    end

    function acb()
        spawn(function()
            while task.wait() and getgenv().autoclickblock do
                local blocks = game:GetService("Workspace").Plots[plot].Merging
                local highestValue = nil
                
                for _, value in pairs(blocks:GetChildren()) do
                    if highestValue == nil then
                        highestValue = value
                    elseif highestValue.Name < value.Name then
                        highestValue = value
                    end
                end
                
                local args = {
                    [1] = workspace:WaitForChild("Plots"):WaitForChild(plot):WaitForChild("Merging"):WaitForChild(highestValue.Name)
                }
                
                game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("ClickBlock"):FireServer(unpack(args))
                wait(0.1)
            end 
        end)
    end

    function ab()
        spawn(function()
            while task.wait() and getgenv().autoobby do
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Obbies.Easy.Win.CFrame
                wait(1)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Obbies.Hard.Win.CFrame
                break
            end
        end)
    end
    
        function rb()
        spawn(function()
            while task.wait() and getgenv().autoobby do
                game:GetService("ReplicatedStorage").RemoteEvents.Rebirth:FireServer()
            end
        end)
    end
    
            function up1()
        spawn(function()
            while task.wait() and getgenv().capkhoi do
                -- Script generated by TurtleSpy, made by Intrer#0421

game:GetService("ReplicatedStorage").RemoteEvents.Upgrade:FireServer(workspace.Plots.Plot4,"LowestSpawn")
            end
        end)
    end

function up2()
        spawn(function()
            while task.wait() and getgenv().maxkhoi do
                game:GetService("ReplicatedStorage").RemoteEvents.Rebirth:FireServer()
            end
        end)
    end
    
    function up3()
        spawn(function()
            while task.wait() and getgenv().thoigian do
                -- Script generated by TurtleSpy, made by Intrer#0421

game:GetService("ReplicatedStorage").RemoteEvents.Upgrade:FireServer(workspace.Plots.Plot4,"SpawnTime")
            end
        end)
    end

    -- GUI
    Window:Toggle({
        Text = "Auto Merge",
        Callback = function(bool)
            getgenv().AutoMerge = bool
            if bool then
                am();
            end 
        end
    })

    Window:Toggle({
        Text = "Auto Click Block",
        Callback = function(bool)
        getgenv().autoclickblock = bool
            if bool then
                acb();
            end
        end
    })

    Window:Toggle({
        Text = "Auto Obby",
        Callback = function(bool)
        getgenv().autoobby = bool
            if bool then
                ab();
            end
        end
    })
    
        Window:Toggle({
        Text = "Auto Rebirth",
        Callback = function(bool)
        getgenv().autorebirth = bool
            if bool then
                rb();
            end
        end
    })
end

        Window:Toggle({
        Text = "Auto Buy Time Spawn",
        Callback = function(bool)
        getgenv().thoigian = bool
            if bool then
                up3();
            end
        end
    })
end

        Window:Toggle({
        Text = "Auto Buy Thêm Sờ lót cho block",
        Callback = function(bool)
        getgenv().maxkhoi = bool
            if bool then
                up2();
            end
        end
    })
end

        Window:Toggle({
        Text = "Auto Buy Lê Vờ Cho Block",
        Callback = function(bool)
        getgenv().capkhoi = bool
            if bool then
                up1();
            end
        end
    })
end
